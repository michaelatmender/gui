    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.6.1",
    "babel-eslint": "^10.0.3",
    "babel-jest": "~24.8.0",
    "babel-loader": "^8.0.6",
    "child_process": "^1.0.2",
    "clean-webpack-plugin": "^2.0.2",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "^2.1.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "core-js": "^3.2.1",
    "eslint": "^5.16.0",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-react": "^7.14.3",
    "file-loader": "^3.0.1",
    "gulp": "^4.0.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "~24.8.0",
    "jest-environment-jsdom-fifteen": "1.0.0",
    "jest-resolve": "~24.8.0",
    "jest-watch-typeahead": "0.4.0",
    "less": "^3.10.3",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.8.0",
    "postcss-loader": "^3.0.0",
    "react-test-renderer": "~16.8.6",
    "resolve-url-loader": "^3.1.0",
    "url-loader": "^1.1.2",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.8",
    "yarn": "^1.17.3"
  },
  "browserslist": [
    ">0.2%",
    "last 5 versions",
    "not dead",
    "not op_mini all"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mendersoftware/gui.git"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "setupFilesAfterEnv": [
      "./tests/setupTests.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js",
      "<rootDir>/src/**/*.{spec,test}.js"
    ],
    "testEnvironment": "jest-environment-jsdom-fifteen",
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.(css|less)$": "<rootDir>/tests/cssTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.js$",
      "^.+\\.module\\.(css|less|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "js"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "scripts": {
    "build": "webpack --mode production ./src/js/main.js",
    "disclaim": "yarn licenses generate-disclaimer > ./dist/disclaimer.txt",
    "lint": "eslint src",
    "lint-fix": "eslint --fix src",
    "test": "node test.js",
    "watch": "webpack --watch --mode development ./src/js/main.js"
  },
  "author": "Michael Clelland",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mendersoftware/gui/issues"
  },
  "homepage": "https://github.com/mendersoftware/gui#readme"
}
